# Application
APP_APP_NAME=FastAPI Boilerplate
APP_ENVIRONMENT=development
APP_DEBUG=true
APP_VERSION=0.1.0

# Server
APP_HOST=0.0.0.0
APP_PORT=8000

# Docs
APP_DOCS_URL=/docs
APP_REDOC_URL=/redoc
APP_OPENAPI_URL=/openapi.json

# CORS
# Use '*' to allow all, or a comma-separated list of origins
APP_CORS_ALLOW_ORIGINS=*
APP_CORS_ALLOW_CREDENTIALS=true
APP_CORS_ALLOW_METHODS=*
APP_CORS_ALLOW_HEADERS=*

# Telemetry
APP_SENTRY_DSN=
